{
  "name": "@conference/apply",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:emulator": "vite --mode emulator",
    "emulator": "firebase emulators:start",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc -b",
    "deploy": "pnpm build && firebase deploy --only hosting:checkin-ba4b8-apply",
